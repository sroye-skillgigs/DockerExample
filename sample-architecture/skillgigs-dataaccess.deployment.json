{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "skillgigs-dataacess",
    "labels": {
      "k8s-app": "skillgigs-dataacess"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "k8s-app": "skillgigs-dataacess"
      }
    },
    "template": {
      "metadata": {
        "name": "skillgigs-dataacess",
        "labels": {
          "k8s-app": "skillgigs-dataacess"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "skillgigs-dataacess",
            "image": "skillgigstech/skillgigs-dataacess-api:v.0.0.0.1",
            "resources": {},
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "imagePullPolicy": "Always",
            "securityContext": {
              "privileged": false
            }
          }
        ],
        "imagePullSecrets": [
          {
            "name": "skillgigstech"
          }
        ],
        "restartPolicy": "Always",
        "terminationGracePeriodSeconds": 30,
        "dnsPolicy": "ClusterFirst",
        "securityContext": {},
        "schedulerName": "default-scheduler"
      }
    },
    "strategy": {
      "type": "RollingUpdate",
      "rollingUpdate": {
        "maxUnavailable": "25%",
        "maxSurge": "25%"
      }
    },
    "revisionHistoryLimit": 10,
    "progressDeadlineSeconds": 600
  }
}
